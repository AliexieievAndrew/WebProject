<?xml version="1.0" encoding="UTF-8"?>
<flow xmlns="http://www.springframework.org/schema/webflow"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.springframework.org/schema/webflow
                          http://www.springframework.org/schema/webflow/spring-webflow.xsd">



    <!--its the start state-->
    <!--flows/  { FLOW ID ("/register" from navbar)}  /signup-personal-->
    <view-state id="personal" view="flows/register/signup-personal"></view-state>
    


    <!--end state-->
    <!--id="home" take from global-transitions ... to="home"-->
    <end-state id="home" view ="externalRedirect:contextRelative:/"/>

    <!--Global transition-->
    <!--var home getting from signup-personal.ftl Navigation-->
    <global-transitions>
        <transition on="home" to="home"/>
    </global-transitions>

</flow>